{
  "plugins": [
    ["babel-plugin-root-import", {
      "rootPathPrefix": "~",
      "rootPathSuffix": "src",
    }],
    ["@babel/plugin-proposal-decorators", {
        "legacy": true,
      },
    ],
    ["@babel/plugin-proposal-class-properties", {"loose": true}],
    ["@babel/transform-runtime"],
  ],
  "presets": ["@babel/preset-env", "@babel/preset-react"]
}
